{
  "name": "html2sketch",
  "version": "0.4.4",
  "author": "arvinxx(arvinx@foxmail.com)",
  "description": "parser HTML to Sketch",
  "keywords": [
    "html",
    "sketch",
    "design",
    "c2d"
  ],
  "homepage": "https://github.com/ant-design/html2sketch",
  "bugs": {
    "url": "https://github.com/ant-design/html2sketch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/html2sketch.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "scripts": {
    "start": "dumi dev",
    "build": "father-build && webpack",
    "---------": "",
    "publish": "npm publish",
    "publish:next": "npm publish --tag next",
    "---------    ": "",
    "site:preview": "PREVIEW=1 dumi build",
    "site:build": "dumi build",
    "site:publish": "dumi build && gh-pages -d ./dist",
    "--------- ": "",
    "test": "umi-test",
    "test:update": "UPDATE=1 umi-test --update-snapshot",
    "test:coverage": "umi-test --coverage",
    "---------  ": "",
    "e2e": "npm run e2e:build && npm run e2e:online",
    "e2e:build": "UMI_ENV=e2e dumi build",
    "e2e:online": "ONLINE=1 jest --config=jest.e2e.config.js",
    "e2e:local": "jest --config=jest.e2e.config.js",
    "e2e:update": "UPDATE=1 ONLINE=1 jest --config=jest.e2e.config.js --update-snapshot",
    "---------   ": "",
    "clean": "rm -rf es lib dist build coverage e2e/dist src/.umi* .eslintcache",
    "lint": "npm run lint-eslint",
    "tsc": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "prepublishOnly": "npm run test && npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "prettier": "prettier -c --write \"**/**\"",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@sketch-hq/sketch-file-format-ts": "5.1.1",
    "color": "^3.1.2",
    "css": "^3.0.0",
    "svg-pathdata": "^5.0.5",
    "svgo-browser": "^1.3.7",
    "svgson": "^4.1.0",
    "transformation-matrix": "^2.5.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@ant-design/charts": "^0.9.13",
    "@ant-design/pro-card": "^1.3.1",
    "@ant-design/pro-skeleton": "^1.0.0-beta.7",
    "@ant-design/pro-table": "^2.6.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^9.1.2",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.7",
    "@types/color": "^3.0.1",
    "@types/css": "0.0.31",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.3",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.1",
    "@types/uuid": "^8.3.0",
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.10",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.2",
    "commitizen": "^4.2.1",
    "commitlint-config-cz": "^0.13.1",
    "concurrently": "^5.3.0",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.0",
    "cz-customizable": "^6.2.1",
    "dumi": "^1.0.13",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.2.1",
    "father-build": "^1.18.1",
    "fs-extra": "^9.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.0.0",
    "jest": "^26.4.2",
    "jest-electron": "^0.1.11",
    "konva": "^7.0.3",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "np": "^6.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "puppeteer": "^5.2.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-json-view": "^1.19.1",
    "react-konva": "^16.13.0-5",
    "stylelint": "^13.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "umi": "^3.0.0",
    "umi-plugin-githubpages": "^2.0.0",
    "umi-test": "^1.9.6",
    "umi-types": "^0.5.11",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
