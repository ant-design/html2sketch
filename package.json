{
  "name": "html2sketch",
  "version": "1.0.0-beta.16",
  "author": "arvinxx(arvinx@foxmail.com)",
  "description": "parser HTML to Sketch",
  "keywords": [
    "html",
    "sketch",
    "design",
    "c2d"
  ],
  "homepage": "https://github.com/ant-design/html2sketch",
  "bugs": {
    "url": "https://github.com/ant-design/html2sketch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/html2sketch.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "scripts": {
    "start": "umi dev --dumi",
    "build": "father build",
    "---------    ": "",
    "docs:preview": "PREVIEW=1 umi build  --dumi",
    "docs:build": "umi build  --dumi",
    "docs:publish": "umi build  --dumi && gh-pages -d ./dist",
    "--------- ": "",
    "test": "umi test",
    "test:update": "UPDATE=1 umi test --update-snapshot",
    "test:coverage": "umi test --coverage",
    "---------   ": "",
    "release": "semantic-release",
    "release:local": "semantic-release  --no-ci",
    "---------     ": "",
    "clean": "rm -rf es lib dist build coverage e2e/dist src/.umi* .eslintcache",
    "ci": "npm run lint && npm run tsc",
    "lint": "npm run lint-eslint",
    "tsc": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "prettier": "prettier -c --write \"**/**\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@sketch-hq/sketch-file-format-ts": "^6",
    "color": "^3.1.2",
    "css": "^3.0.0",
    "svg-pathdata": "^5.0.5",
    "svgo-browser": "^1.3.7",
    "svgson": "^4.1.0",
    "transformation-matrix": "^2.11.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@ant-design/charts": "^1.3.6",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.3.1",
    "@ant-design/pro-skeleton": "^1.0.0-beta.7",
    "@ant-design/pro-table": "^2.6.3",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/color": "^3.0.1",
    "@types/css": "0.0.31",
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.3",
    "@types/lodash": "^4.14.184",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/uuid": "^8.3.0",
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-dumi": "^1.1.40",
    "@umijs/preset-react": "^1.8.6",
    "@umijs/test": "^3.4.15",
    "antd": "^4.19.5",
    "commitlint": "^17.1.2",
    "commitlint-config-gitmoji": "^2.2.5",
    "concurrently": "^7.3.0",
    "copy-to-clipboard": "^3.3.2",
    "cross-env": "^7.0.0",
    "eslint": "^7.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.2.1",
    "father": "^4.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-electron": "^0.1.11",
    "lint-staged": "^10.0.2",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-json-view": "^1.19.1",
    "semantic-release": "^19.0.3",
    "semantic-release-config-gitmoji": "^1.2.4",
    "stylelint": "^13.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.8.2",
    "umi": "^3.5.21",
    "umi-plugin-githubpages": "^2.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
