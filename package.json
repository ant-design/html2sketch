{
  "name": "html2sketch",
  "version": "1.0.0-beta.9",
  "author": "arvinxx(arvinx@foxmail.com)",
  "description": "parser HTML to Sketch",
  "keywords": [
    "html",
    "sketch",
    "design",
    "c2d"
  ],
  "homepage": "https://github.com/ant-design/html2sketch",
  "bugs": {
    "url": "https://github.com/ant-design/html2sketch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/html2sketch.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "scripts": {
    "start": "umi dev --dumi",
    "build": "father-build && webpack",
    "---------    ": "",
    "docs:preview": "PREVIEW=1 umi build  --dumi",
    "docs:build": "umi build  --dumi",
    "docs:publish": "umi build  --dumi && gh-pages -d ./dist",
    "--------- ": "",
    "test": "umi test",
    "test:update": "UPDATE=1 umi test --update-snapshot",
    "test:coverage": "umi test --coverage",
    "---------  ": "",
    "e2e": "npm run e2e:build && npm run e2e:online",
    "e2e:build": "UMI_ENV=e2e umi build  --dumi",
    "e2e:online": "ONLINE=1 jest --config=jest.e2e.config.js",
    "e2e:local": "jest --config=jest.e2e.config.js",
    "e2e:update": "UPDATE=1 ONLINE=1 jest --config=jest.e2e.config.js --update-snapshot",
    "---------   ": "",
    "release": "semantic-release",
    "---------     ": "",
    "clean": "rm -rf es lib dist build coverage e2e/dist src/.umi* .eslintcache",
    "lint": "npm run lint-eslint",
    "tsc": "tsc -p tsconfig-check.json",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ",
    "prettier": "prettier -c --write \"**/**\"",
    "commit": "git-cz"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@sketch-hq/sketch-file-format-ts": "5.1.1",
    "color": "^3.1.2",
    "css": "^3.0.0",
    "svg-pathdata": "^5.0.5",
    "svgo-browser": "^1.3.7",
    "svgson": "^4.1.0",
    "transformation-matrix": "^2.5.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@ant-design/charts": "^0.9.13",
    "@ant-design/pro-card": "^1.3.1",
    "@ant-design/pro-skeleton": "^1.0.0-beta.7",
    "@ant-design/pro-table": "^2.6.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/classnames": "^2.2.7",
    "@types/color": "^3.0.1",
    "@types/css": "0.0.31",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.3",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/uuid": "^8.3.0",
    "@umijs/fabric": "^2.2.2",
    "@umijs/preset-dumi": "^1.1.16",
    "@umijs/preset-react": "^1.8.6",
    "@umijs/test": "^3.4.15",
    "@umijs/types": "^3.4.15",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.2",
    "commitlint": "^12.1.1",
    "commitlint-config-gitmoji": "^2.2.5",
    "concurrently": "^5.3.0",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.0",
    "eslint": "^7.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.2.1",
    "father-build": "^1.18.1",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-electron": "^0.1.11",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "puppeteer": "^9.1.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-json-view": "^1.19.1",
    "semantic-release": "^17.4.2",
    "semantic-release-config-gitmoji": "^1.2.4",
    "stylelint": "^13.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.2.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4",
    "umi": "^3.4.15",
    "umi-plugin-githubpages": "^2.0.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
